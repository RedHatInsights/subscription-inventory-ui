{
  "name": "subscription-inventory",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "analyze": "npm run build:prod && source-map-explorer 'dist/js/*.js'",
    "build": "webpack --config config/prod.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "format": "prettier \"src/**/*.{js,tsx,html,scss}\" --write",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "lint:staged": "pretty-quick --staged && lint-staged",
    "nightly": "npm run deploy",
    "postinstall": "rm -rf .cache",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:beta": "BETA=true NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "test": "TZ=America/New_York jest --verbose --no-cache",
    "test:watch": "TZ=America/New_York jest --watch",
    "verify": "npm-run-all build lint test",
    "prepare": "husky install"
  },
  "overrides": {
    "glob-parent": "^6.0.1",
    "hosted-git-info": "^3.0.8",
    "url-parse": "^1.5.3",
    "moment": "2.29.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,ts,tsx,html}": "eslint --cache --fix"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/stories/*",
      "!src/App.js",
      "!src/AppEntry.js",
      "!src/bootstrap-dev.js",
      "!src/bootstrap.js",
      "!src/entry-dev.js",
      "!src/entry.js",
      "!src/Routes.tsx",
      "!src/store/index.js"
    ],
    "coverageDirectory": "./coverage/",
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "clearMocks": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "jsdom"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@redhat-cloud-services)",
    "/node_modules/(?!@patternfly)"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@patternfly/patternfly": "^5.0.0",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-table": "^5.0.0",
    "@redhat-cloud-services/frontend-components": "^4.2.3",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.4",
    "@unleash/proxy-client-react": "^4.0.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "classnames": "^2.5.1",
    "escape-string-regexp": "^5.0.0",
    "jest-fetch-mock": "^3.0.3",
    "js-cookie": "^3.0.5",
    "load-json-file": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "redux": "^4.2.1",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "^6.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "unleash-proxy-client": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-flow": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.4.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.3",
    "@redhat-cloud-services/frontend-components-config": "^5.0.5",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.25",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-dual-import": "1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "bdd-lazy-var": "^2.6.1",
    "cross-fetch": "^4.0.0",
    "css-loader": "^6.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fishery": "^2.2.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.3.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "4.1.5",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.0",
    "pretty-quick": "^4.0.0",
    "prop-types": "15.8.1",
    "sass-loader": "^14.1.1",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-scss": "^5.2.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4"
  },
  "insights": {
    "appname": "subscription-inventory"
  }
}
